# turn off shutdown mode
0000 1100 0000 0001
# set scan limit to display all rows
0000 1011 0000 0111
# disable decoding
0000 1001 0000 0000

# draw picture
0000 0001 00000000
0000 0010 00011110
0000 0011 11111110
0000 0100 11111110
0000 0101 11111110
0000 0110 11110010
0000 0111 00000010
0000 1000 00000010
